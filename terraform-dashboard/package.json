{
  "name": "terraform-dashboard",
  "version": "1.0.0",
  "description": "A modern web application for managing Terraform infrastructure with a clean GUI interface",
  "main": "index.js",
  "scripts": {
    "setup": "npm run setup:backend && npm run setup:frontend",
    "setup:backend": "cd backend && npm install",
    "setup:frontend": "cd frontend && npm install",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm start",
    "api": "concurrently \"npm run api:backend\" \"npm run dev:frontend\"",
    "api:backend": "cd backend && npm run api",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && serve -s build -l 3000",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "terraform",
    "aws",
    "infrastructure",
    "dashboard",
    "react",
    "nodejs",
    "typescript"
  ],
  "author": "Terraform Dashboard Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/terraform-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/terraform-dashboard/issues"
  },
  "homepage": "https://github.com/your-org/terraform-dashboard#readme",
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.844.0",
    "@aws-sdk/client-ec2": "^3.844.0",
    "@aws-sdk/client-eks": "^3.844.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.10.0",
    "node-fetch": "^3.3.2"
  }
}
